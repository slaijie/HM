  "metadata": {
    "name": ["h3 > a > span","meta[property=og:novel:book_name]@attr->content"],
    "author": ["p:nth-child(1) > span:nth-child(2)","meta[property=og:novel:author]@attr->content"],
    "cover": ["a > img@attr->src","meta[property=og:image]@attr->content"],
    "summary": ["div.result-game-item-detail > p","meta[property=og:description]@attr->content"],
    "category": ["p:nth-child(2) > span:nth-child(2)","meta[property=og:novel:category]@attr->content"],
    "status": ["meta[property=og:novel:status]@attr->content"],
    "update": ["p:nth-child(3) > span:nth-child(2)","meta[property=og:novel:update_time]@attr->content"],
    "lastChapter": ["p:nth-child(4) > a","meta[property=og:novel:latest_chapter_name]@attr->content"],
    "link": ["h3 > a@attr->href"],
    "catalog": ["div.bottem1 > a:nth-child(2)@attr->href"]
  },
  "catalog": {
    "list": "#list > dl > dd",
    "orderBy": 0,
    "chapter": {
      "name": "a",
      "link": "a@attr->href"
    }
  },
  "content": {
    "text": "#content"
  },
  "search": {
    "link": "https://www.zwdu.com/search.php?keyword=${key}",
    "list": "div.result-list > div"
  } 
}
