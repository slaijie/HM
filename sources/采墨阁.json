{
  "name": "采墨阁",
  "version": 101,
  "category": 1,
  "url": "https://www.caimoge.com",
  "charset": "utf-8",
  "metadata": {
    "name": ["dd:nth-child(2) > h3 > a","meta[property=og:novel:book_name]@attr->content"],
    "author": ["dd:nth-child(3) > span:nth-child(1)","meta[property=og:novel:author]@attr->content"],
    "cover": ["dt > a > img@attr->src","meta[property=og:image]@attr->content"],
    "summary": ["dd.book_des","meta[property=og:novel:description]@attr->content"],
    "category": ["dd:nth-child(3) > span:nth-child(3)","meta[property=og:novel:category]@attr->content"],
    "status": ["dd:nth-child(3) > span:nth-child(2)","meta[property=og:novel:status]@attr->content"],
    "update": ["dd:nth-child(5) > span","meta[property=og:novel:update_time]@attr->content"],
    "lastChapter": ["dd:nth-child(5) > a","meta[property=og:novel:latest_chapter_name]@attr->content"],
    "link": ["dd:nth-child(2) > h3 > a@attr->href"],
    "catalog": ["#pdu@attr->href"]
  },
  "catalog": {
    "list": "#readerlists > ul > li",
    "orderBy": 0,
    "chapter": {
      "name": "a",
      "link": "a@attr->href"
    }
  },
  "content": {
    "text": "#content > p"
  },
  "search": {
    "link": "https://www.caimoge.com/search.html@post->searchkey=${key}",
    "list": "#sitembox  > dl"
  } 
}
